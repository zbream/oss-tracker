{
  "extends": [
    "zbream-standards/config/angular/tslint"
  ],
  "rules": {
    "component-selector": {
      "options": [
        "element",
        ["oss"],
        "kebab-case"
      ]
    },
    "directive-selector": {
      "options": [
        "attribute",
        ["oss"],
        "camelCase"
      ]
    },
    "pipe-prefix": {
      "options": [
        "oss"
      ]
    }
  }
}
